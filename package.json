{
  "name": "showcase",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true
  },
  "scripts": {
    "start-pro": "egg-scripts start --title=egg-ts-startkit",
    "start": "egg-scripts start --daemon --title=egg-ts-startkit",
    "stop": "egg-scripts stop --title=egg-ts-startkit",
    "dev": "DEBUG='egg-connector-remote:accessContext:acl' egg-bin dev -r egg-ts-helper/register",
    "dev-js": "egg-bin dev",
    "debug": "egg-bin debug -r egg-ts-helper/register",
    "test-local": "egg-bin test -r egg-ts-helper/register",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov -r egg-ts-helper/register",
    "tsc": "ets && tsc -p tsconfig.json",
    "tsc:w": "ets && tsc -p tsconfig.json -w",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint .",
    "clean": "ets clean",
    "migrate:new": "egg-sequelize migration:create",
    "migrate:up": "egg-sequelize db:migrate",
    "migrate:down": "egg-sequelize db:migrate:undo",
    "migrate:rest": "egg-sequelize db:migrate:undo:all && egg-sequelize db:migrate"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dataloader": "^1.4.0",
    "egg": "^2.6.1",
    "egg-connector-remote": "^1.0.6",
    "egg-graphql": "^2.1.0",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^3.1.4",
    "egg-swagger": "^1.1.2",
    "mysql2": "^1.5.3",
    "sqlite3": "^4.0.0",
    "uid-promise": "^1.0.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/debug": "^0.0.30",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/sequelize": "^4.27.20",
    "@types/supertest": "^2.0.0",
    "@types/uuid": "^3.4.3",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.6.2",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "egg-ts-helper": "^1.4.2",
    "tslib": "^1.9.0",
    "tslint": "^4.0.0",
    "typescript": "^2.8.1"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "",
  "license": "MIT"
}
